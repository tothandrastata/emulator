# GVN-MMU Emulator Configuration Example
# Copy this file to .env and modify as needed

# Device Identity
DEVICE_MODEL=GVN-MMU
PRODUCT_NAME=GVN-MMU
PART_NUMBER=91000000
MANUFACTURER_NAME=Lightware Visual Engineering
SERIAL_NUMBER=EMULATOR_GVN
PACKAGE_VERSION=v1.0.0

# Network Configuration
LW3_HOST=0.0.0.0
LW3_PORT=6107                          # LW3 protocol port (default: 6107)

# Web UI Configuration
WEB_UI_ENABLED=true                    # Enable web-based control interface
WEB_UI_HOST=0.0.0.0                    # Host binding for web UI (0.0.0.0 = all interfaces)
WEB_UI_PORT=8082                       # HTTP port for web UI (default: 8082)

# Matrix Configuration
MATRIX_SIZE=5                          # Number of endpoints (X1-X5)
