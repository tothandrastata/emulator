# TPN-MMU Emulator Configuration Example
# Copy this file to .env and modify as needed

# Device Identity
DEVICE_MODEL=TPN-MMU-X100
PRODUCT_NAME=TPN-MMU-X100
PART_NUMBER=91710013
MANUFACTURER_NAME=Lightware Visual Engineering
SERIAL_NUMBER=EMULATOR
PACKAGE_VERSION=v0.0.0

# Network Configuration
LW3_HOST=0.0.0.0
LW3_PORT=7107                          # LW3 protocol port (default: 7107)

# Web UI Configuration
WEB_UI_ENABLED=true                    # Enable web-based control interface
WEB_UI_HOST=0.0.0.0                    # Host binding for web UI (0.0.0.0 = all interfaces)
WEB_UI_PORT=8081                       # HTTP port for web UI (default: 8081)

# Matrix Configuration
MATRIX_SIZE=3                          # Number of inputs/outputs (3x3 matrix)
